{
  "name": "review-code",
  "displayName": "review-code",
  "description": "",
  "version": "0.1.3",
  "engines": {
    "vscode": "^1.73.0"
  },
  "icon": "logo128.png",
  "publisher": "hritik2002",
  "repository": {
    "url": "https://github.com/hritik2002/review-code"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:review-code.helloWorld"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "review-code.helloWorld",
        "title": "Review Code"
      },
      {
        "command": "extension.setGeminiApiKey",
        "title": "Set Gemini API Key"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Gemini API Configuration",
      "properties": {
        "gemini.apiKey": {
          "type": "string",
          "default": "",
          "description": "Enter your Gemini API key here."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.73.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@google/generative-ai": "^0.16.0",
    "axios": "^1.3.3"
  }
}
